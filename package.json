{
  "name": "bizpickr-auth-service",
  "version": "1.0.0",
  "description": "Authentication microservice for Biz Picker",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "nodemon src/index.js",
    "dev:server": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "cross-env NODE_ENV=production node src/index.js",
    "test": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest",
    "test:watch": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest --watch",
    "test:coverage": "cross-env NODE_ENV=test node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "docker:build": "docker build -t auth-service .",
    "docker:run": "docker run -p 3001:3001 auth-service",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "migrate": "sequelize db:migrate",
    "migrate:undo": "sequelize db:migrate:undo:all",
    "seed": "sequelize db:seed:all",
    "db:reset": "npm run migrate:undo:all && npm run migrate && npm run seed",
    "setup:dev": "node scripts/setup-dev.js",
    "start:dev": "node scripts/start-dev.js"
  },
  "author": "neel networks",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/neelnetworks/bizpickr-auth-service.git"
  },
  "keywords": [
    "authentication",
    "microservice",
    "jwt",
    "oauth",
    "2fa",
    "express",
    "nodejs",
    "javascript",
    "mysql",
    "mongodb",
    "redis"
  ],
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "amqplib": "^0.10.8",
    "async-retry": "^1.3.3",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "joi": "^17.13.3",
    "jose": "^6.0.12",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.4",
    "morgan": "^1.10.0",
    "mysql2": "^3.14.2",
    "nodemailer": "^7.0.5",
    "opossum": "^9.0.0",
    "qrcode": "^1.5.4",
    "sequelize": "^6.37.7",
    "speakeasy": "^2.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^30.0.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.3",
    "supertest": "^7.0.0"
  }
}
